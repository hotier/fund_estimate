{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["hkg1"],
  "crons": [
    {
      "path": "/api/cron/update-funds",
      "schedule": "*/5 1-7 * * 1-5"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://fund-estimate.vercel.app"
  }
}
